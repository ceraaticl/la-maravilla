                                    </tr>
                                    <tr>
                                        <td class="r-o">{{cama}}:</td>
                                        <td>{{camadata1}}</td>
                                    </tr>
                                    <tr>
                                        <td class="r-o">{{servicios}}:</td>
                                        <td>{{serviciosdata1}}</td>
                                    </tr>
                                </tbody>
                            </table>
