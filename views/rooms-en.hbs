    <!-- Breadcrumb Section Begin -->
    <div class="breadcrumb-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text">
                        <h2>{{habitaciones}}</h2>
                        <div class="bt-option">
                            <a href="/home-en">{{inicio}}</a>
                            <span>{{habitaciones}}</span>
                        </div>
                    </div>
                </div>
                <br>
                <div class="container" style="padding-left: 10%; padding-right: 10%; padding-top: 2%;">

                    <div class="col-lg-12">
                        <p style="text-align:center">{{info}}</p>
                    </div>
                </div>

            </div>
        </div>
    </div>
